<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee http://www.w3.org/2001/XMLSchema-instance  
			http://www.springmodules.org/schema/cache/springmodules-cache.xsd 
			http://www.springmodules.org/schema/cache/springmodules-ehcache.xsd" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
	<display-name>post</display-name>
  <welcome-file-list>
    <welcome-file>front.html</welcome-file>
    <welcome-file>front.htm</welcome-file>
    <welcome-file>front.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
		<servlet-name>PostRemove</servlet-name>
		<servlet-class>edit.data.PostRemove</servlet-class>
	</servlet>
	<servlet>		
		<servlet-name>PostQuery</servlet-name>
		<servlet-class>edit.data.PostQuery</servlet-class>
	</servlet>
	<servlet>		
		<servlet-name>PostModify</servlet-name>
		<servlet-class>edit.data.PostModify</servlet-class>
		<init-param>
  				<param-name>charset</param-name>
  				<param-value>utf-8</param-value>
  		</init-param>
	</servlet>
	<servlet>
		<servlet-name>PostAdd</servlet-name>
		<servlet-class>edit.data.PostAdd</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PostRemove</servlet-name>
		<url-pattern>/PostRemove</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PostQuery</servlet-name>
		<url-pattern>/PostQuery</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PostModify</servlet-name>
		<url-pattern>/PostModify</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PostAdd</servlet-name>
		<url-pattern>/PostAdd</url-pattern>
	</servlet-mapping>
</web-app>