<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee http://www.w3.org/2001/XMLSchema-instance  
			http://www.springmodules.org/schema/cache/springmodules-cache.xsd 
			http://www.springmodules.org/schema/cache/springmodules-ehcache.xsd" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
	<display-name>trade_change</display-name>
  <welcome-file-list>
    <welcome-file>front.html</welcome-file>
    <welcome-file>front.htm</welcome-file>
    <welcome-file>front.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
		<servlet-name>Remove</servlet-name>
		<servlet-class>data.change.Remove</servlet-class>
	</servlet>
	<servlet>		
		<servlet-name>Query</servlet-name>
		<servlet-class>data.change.Query</servlet-class>
	</servlet>
	<servlet>		
		<servlet-name>Modify</servlet-name>
		<servlet-class>data.change.Modify</servlet-class>
		<init-param>
  				<param-name>charset</param-name>
  				<param-value>utf-8</param-value>
  		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>Remove</servlet-name>
		<url-pattern>/Remove</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Query</servlet-name>
		<url-pattern>/Query</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Modify</servlet-name>
		<url-pattern>/Modify</url-pattern>
	</servlet-mapping>
</web-app>